or (DESCRIPTION like '%Статья 93%' and DESCRIPTION like '%п.25%')
) and document_id not in (select f.document_id from docflag f , docflagtype ft where f.docflagtype_id=ft.id  and f.docflagtype_id in (213,214,307) )
/* BIND AND customer_id IN ($CUST_ID) */
and dispstatus_id not in (-1,11,24,7,9)
) and paydate  BETWEEN ? AND ?   group by paydate,contract_id)
) B
on A.contract_id = B.id group by A.contract_id,paydate,contract_id,customername,doc_number,status,kod]]></xml-property>
